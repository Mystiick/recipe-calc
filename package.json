{
  "name": "recipe-calc",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && electron ./dist/main.js",
    "start-fast": "electron ./dist/main.js",
    "build": "tsc && npm run copy",
    "watch": "tsc -w",
    "copy": "npm run copy-html && npm run copy-css && npm run copy-data",
    "copy-html": "xcopy \".\\src\\*.html\" \".\\dist\\src\\\" /s /y",
    "copy-css": "xcopy \".\\src\\*.css\" \".\\dist\\src\\\" /s /y",
    "copy-data": "xcopy \".\\data\\*\" \".\\dist\\data\\\" /s /y"
  },
  "_moduleAliases": {
    "@models": "dist/src/models/",
    "@services": "dist/src/services/",
    "also-update": "tsconfig.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.5.3",
    "electron": "^11.1.0",
    "jquery": "^3.5.1",
    "module-alias": "^2.2.2",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "typescript": "4.1.3"
  }
}
